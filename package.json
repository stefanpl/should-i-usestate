{
  "name": "nextjs-with-typescript",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint-base": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint": "npm run lint-base -- --max-warnings 0",
    "lint-fix": "npm run lint-base -- --fix",
    "prettify": "npx prettier --write .",
    "typecheck": "npx tsc --noEmit",
    "unit-test": "echo 'no unit tests implemented so far'",
    "full-check": "npm run typecheck && npm run lint && npm run prettify && npm run test && echo \"âœ… All good!\"",
    "test": "npm run unit-test",
    "prod": "npm run build",
    "watch": "npm run dev",
    "production": "npm run build",
    "serve": "npm run start"
  },
  "dependencies": {
    "@emotion/cache": "latest",
    "@emotion/react": "latest",
    "@emotion/server": "latest",
    "@emotion/styled": "latest",
    "@mui/icons-material": "latest",
    "@mui/material": "latest",
    "@nvon/eslint-config": "^1.15.2",
    "next": "latest",
    "react": "latest",
    "react-dom": "latest"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@types/react": "latest",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "typescript": "latest"
  }
}
